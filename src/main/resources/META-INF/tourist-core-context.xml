<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd">

	<bean id="tourist-parent" class="io.diepet.labs.tourist.core.TouristImpl"
		abstract="true">
		<property name="cameraRollFactory">
			<bean class="io.diepet.labs.tourist.core.CameraRollFactoryImpl" />
		</property>
		<property name="tourEventListenerSet">
			<set><!-- Inject an empty set -->
			</set>
		</property>
	</bean>

</beans>